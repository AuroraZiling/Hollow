    Padding="25"
    x:Class="Hollow.Views.Pages.GameSettings"
    x:DataType="pages:GameSettingsViewModel"
    xmlns="https://github.com/avaloniaui"
    xmlns:languages="clr-namespace:Hollow.Languages"
